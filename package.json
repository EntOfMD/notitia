{
	"name": "notitia",
	"version": "0.2.2",
	"description": "a tool for product owners to get feedback from customers",
	"main": "index.js",
	"engines": {
		"node": "10.6.0",
		"npm": "6.1.0"
	},
	"scripts": {
		"start": "node index.js",
		"heroku-postbuild": "NPM_CONFIG_PRODUCTION=false npm install --prefix client && npm run build --prefix client",
		"server": "nodemon index.js",
		"client": "npm run start --prefix client",
		"dev": "concurrently \"npm run server\" \"npm run client\""
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/EntOfMD/notitia.git"
	},
	"author": "EntOfMD",
	"license": "ISC",
	"bugs": {
		"url": "https://github.com/EntOfMD/notitia/issues"
	},
	"homepage": "https://github.com/EntOfMD/notitia#readme",
	"dependencies": {
		"@sendgrid/client": "^6.3.0",
		"@sendgrid/helpers": "^6.5.1",
		"@sendgrid/mail": "^6.3.1",
		"body-parser": "^1.18.3",
		"concurrently": "^3.6.0",
		"cookie-session": "^2.0.0-beta.3",
		"express": "^4.16.3",
		"mongoose": "^5.5.1",
		"passport": "^0.4.0",
		"passport-google-oauth20": "^1.0.0",
		"react-redux": "^5.0.7",
		"react-router-dom": "^4.3.1",
		"redux": "^4.0.0",
		"stripe": "^6.3.0"
	}
}
